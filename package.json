{
  "name": "weekly-snippets-reminder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-schema": "node scripts/generate-schema.js",
    "generate-schema:smart": "node scripts/smart-schema-generate.js",
    "generate-schema:force": "node scripts/smart-schema-generate.js --force",
    "dev": "npm run generate-schema:smart && next dev",
    "dev:fresh": "npm run generate-schema:force && next dev",
    "build": "npm run generate-schema:force && npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "node run-basic-tests.js && node __tests__/snippet-creation-integration.test.js",
    "test:watch": "node run-basic-tests.js && node __tests__/snippet-creation-integration.test.js",
    "test:coverage": "node run-basic-tests.js && node __tests__/snippet-creation-integration.test.js",
    "test:ci": "node run-basic-tests.js && node __tests__/snippet-creation-integration.test.js",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "update-oauth-uris": "./scripts/update-oauth-uris.sh",
    "open-oauth-setup": "./scripts/open-oauth-setup.sh",
    "setup-domain": "./scripts/setup-custom-domain.sh",
    "setup-oauth-domain": "./scripts/update-oauth-custom-domain.sh",
    "setup-oauth-2025": "./scripts/update-oauth-2025.sh",
    "oauth-diagnostic": "./scripts/oauth-diagnostic.sh",
    "deep-oauth-debug": "./scripts/deep-oauth-debug.sh",
    "test-oauth-different-account": "./scripts/test-oauth-different-account.sh",
    "fix-production-database": "./scripts/fix-production-database.sh",
    "setup-cloud-sql": "./scripts/setup-cloud-sql.sh",
    "deploy": "./scripts/deploy.sh",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "typecheck:fast": "tsc --project tsconfig.build.json --noEmit --skipLibCheck",
    "typecheck:quick": "node scripts/quick-typecheck.js",
    "check:consistency": "node scripts/check-property-consistency.js",
    "check:contracts": "node scripts/validate-api-contracts.js", 
    "check:templates": "node scripts/check-template-variables.js",
    "check:all": "npm run check:consistency && npm run check:contracts && npm run check:templates",
    "prepare": "husky"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "date-fns": "^4.1.0",
    "googleapis": "^154.0.0",
    "next": "14.2.0",
    "next-auth": "^4.24.0",
    "postcss": "^8.4.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^8.0.7",
    "rehype-highlight": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.4.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
